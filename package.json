{
  "name": "app-mobile-inventory",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "start": "ionic serve --host $IP --port 8080 --public $C9_HOSTNAME --address $IP --dev-logger-port 8081 --livereload-port 8082"
  },
  "dependencies": {
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@auth0/cordova": "^0.3.0",
    "@ionic-native/camera": "^4.6.0",
    "@ionic-native/code-push": "^4.6.0",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/pro": "1.0.20",
    "@ionic/storage": "2.1.3",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "angular2-letter-avatar": "^2.1.1",
    "auth0-js": "^9.3.0",
    "cordova-plugin-camera": "^4.0.2",
    "cordova-plugin-code-push": "^1.11.7",
    "cordova-plugin-customurlscheme": "^4.3.0",
    "cordova-plugin-ionic": "^4.1.3",
    "cordova-plugin-safariviewcontroller": "^1.5.2",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "materialize-css": "^1.0.0-beta",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "typings": "^2.1.1",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.8",
    "es6-shim": "^0.35.3",
    "jasmine": "^3.1.0",
    "typescript": "2.4.2"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-safariviewcontroller": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "{com.inventory.system}",
        "ANDROID_SCHEME": "{com.inventory.system}",
        "ANDROID_HOST": "inventory-system.auth0.com",
        "ANDROID_PATHPREFIX": "/cordova/{com.inventory.system}/callback"
      },
      "cordova-plugin-camera": {},
      "cordova-plugin-code-push": {},
      "cordova-plugin-ionic": {
        "APP_ID": "36217980",
        "CHANNEL_NAME": "Master",
        "UPDATE_METHOD": "background"
      }
    }
  },
  "config": {
    "ionic_source_map_type": "source-map",
    "ionic_webpack": "./config/webpack.config.js",
    "ionic_copy": "./config/copy.config.js"
  }
}
